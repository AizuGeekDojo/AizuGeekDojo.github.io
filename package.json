{
  "name": "geek-web-page",
  "version": "1.0.0",
  "description": "My impressive Nuxt.js project",
  "author": "nozo-moto",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "npm run processmd && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "processmd": "rm -f ./blog/*.json && processmd ./blog/* --stdout --outputDir ./blog/ > ./blog/summary.json",
    "blog-template": "processmd_template_maker --outputDir=./blog/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/sitemap": "^1.3.1",
    "cross-env": "^5.2.1",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-highlight": "^0.2.0",
    "markdown-it-meta": "^0.0.1",
    "markdown-it-table-of-contents": "^0.4.4",
    "nuxt": "^2.15.4",
    "processmd_template_maker": "github:nozo-moto/processmd_template_maker",
    "sass-loader": "^7.3.1",
    "sticky": "^0.8.0",
    "vue-sticky-directive": "0.0.8",
    "vue-sticky-position": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/webpack": "^2.15.4",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "nuxt-fontawesome": "^0.4.0",
    "prettier": "1.14.3",
    "processmd": "^4.6.0"
  }
}
